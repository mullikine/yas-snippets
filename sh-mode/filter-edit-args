# -*- mode: snippet -*-
# name: filter-edit-args
# key: fa
# --
#!/bin/bash
export TTY

a=("$@")

echo 0
cmd "$@" | awk 1

echo 1
set --
cmd "$@" | awk 1

echo 2
set -- "${a[@]}"
cmd "$@" | awk 1

echo 3
eval "set -- $(cmd "$@" | sed "s/ -- .*//")"
b=("$@")
cmd "$@" | awk 1

echo 4
set -- "${a[@]}"
cmd "$@" | awk 1

echo 5
eval "set -- $(cmd "$@" | sed "s/.* --//")"
c=("$@")
cmd "$@" | awk 1

# Copy c
cc=("${c[@]}")